<textcomponents>
	<component id="ID_SC_title">
		<text><![CDATA[<b>Configuring LAN Interfaces</b>]]></text>
	</component>
	<component id="ID_SC" fontFace='Courier New' fontSize="10">
		<text></text>	

		<feedback id="fb"><![CDATA[<span><font size = '10' face="Arial"><span><b>***</b></span>You must enter the exact and full command. Commands that include a name or password are case-sensitive.<span><b>***</b></span></font></span>
		]]></feedback>

		<existingcommands><![CDATA[<font size = '10' face="Arial"><b>Configure the GigabitEthernet 0/0 interface with the IP address '192.168.10.1' and subnet mask '255.255.255.0'. Describe the link as 'LAN-10' and activate the interface.</b></font>]]></existingcommands>
		<command prompt="R1#" id="cmd1" node="start">
			<correctresponse id="cr1" ignoreCase = 'true'><![CDATA[configure terminal]]></correctresponse>
			<deviceresponse id="dr1"><![CDATA[<font size='10'>Enter configuration commands, one per line. End with CNTL/Z.</font>]]></deviceresponse>
			<instruction id="ins1"><![CDATA[<span></span>]]></instruction> 
		</command>
		<command prompt="R1(config)#" id="cmd2">
		    <correctresponse  id="cr2" ignoreCase = 'true'><![CDATA[interface gigabitethernet 0/0]]></correctresponse>
		    <deviceresponse id="dr2"><![CDATA[<span></span>]]></deviceresponse>
			<instruction id="ins2"><![CDATA[<span></span>]]></instruction> 
		</command>
		<command prompt="R1(config-if)#" id="cmd3">
		    <correctresponse id="cr3" ignoreCase = 'true'><![CDATA[ip address 192.168.10.1 255.255.255.0]]></correctresponse>
		    <deviceresponse id="dr3"><![CDATA[<span></span>]]></deviceresponse>
			<instruction id="ins3"><![CDATA[<span></span>]]></instruction> 
		</command>	
		<command prompt="R1(config-if)#" id="cmd4">
		    <correctresponse id="cr4" ignoreCase = 'true'><![CDATA[description LAN-10]]></correctresponse>
		    <deviceresponse id="dr4"><![CDATA[<span></span>]]></deviceresponse>
			<instruction id="ins4"><![CDATA[<span></span>]]></instruction> 
		</command>	
		<command prompt="R1(config-if)#" id="cmd5">
		    <correctresponse id="cr5" ignoreCase = 'true'><![CDATA[no shutdown]]></correctresponse>
		    <deviceresponse id="dr5"><![CDATA[<font size='10'>%LINK-5-CHANGED: Interface GigabitEthernet0/0, changed state to up<br>%LINEPROTO-5-UPDOWN: Line protocol on Interface GigabitEthernet0/0, changed state to up<br>R1(config-if)#exit<br></font>]]></deviceresponse>
		    <instruction  id="ins5"><![CDATA[<font size = '10' face="Arial"><b>Configure the GigabitEthernet 0/1 interface with the IP address '192.168.11.1' and subnet mask '255.255.255.0'. Describe the link as 'LAN-11' and activate the interface.</b></font>]]></instruction>
		</command>
		<command prompt="R1(config)#" id="cmd6">
		    <correctresponse id="cr6" ignoreCase = 'true'><![CDATA[interface gigabitethernet 0/1]]></correctresponse>
		    <deviceresponse id="dr6"><![CDATA[<span></span>]]></deviceresponse>
			<instruction id="ins6"><![CDATA[<span></span>]]></instruction> 
		</command>
		<command prompt="R1(config-if)#"  id="cmd7">
		    <correctresponse  id="cr7" ignoreCase = 'true'><![CDATA[ip address 192.168.11.1 255.255.255.0]]></correctresponse>
		    <deviceresponse id="dr7"><![CDATA[<span></span>]]></deviceresponse>
			<instruction id="ins7"><![CDATA[<span></span>]]></instruction>
		</command>	
		<command prompt="R1(config-if)#"  id="cmd8">
		    <correctresponse id="cr8" ignoreCase = 'true'><![CDATA[description LAN-11]]></correctresponse>
		    <deviceresponse id="dr8"><![CDATA[<span></span>]]></deviceresponse>
			<instruction id="ins8"><![CDATA[<span></span>]]></instruction> 
		</command>	
		<command prompt="R1(config-if)#"  id="cmd9" node="end">
		    <correctresponse  id="cr9" ignoreCase = 'true'><![CDATA[no shutdown]]></correctresponse>
		    <deviceresponse id="dr9"><![CDATA[<font size='10'>%LINK-5-CHANGED: Interface GigabitEthernet0/1, changed state to up<br>%LINEPROTO-5-UPDOWN: Line protocol on Interface GigabitEthernet0/1, changed state to up<br><br></font>]]></deviceresponse>
		    <instruction  id="last"><![CDATA[<font size = '10' face="Arial"><b>You successfully configured the R1 LAN interfaces.</b></font>]]></instruction>
		</command>
		<command prompt=""/>
	</component>
</textcomponents> 